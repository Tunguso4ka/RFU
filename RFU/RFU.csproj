<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <StartupObject>RFU.App</StartupObject>
    <AssemblyVersion>0.1.4.1</AssemblyVersion>
    <FileVersion>0.1.4.1</FileVersion>
    <Version>0.1.4.1</Version>
    <Product>RandomFightsUpdater</Product>
    <Company>Kira Kosova</Company>
    <Authors>Kira Kosova</Authors>
    <ApplicationIcon>favicon (2).ico</ApplicationIcon>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>napfis.pfx</AssemblyOriginatorKeyFile>
  </PropertyGroup>


  <ItemGroup>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>PublicSettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
